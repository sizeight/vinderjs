{
  "name": "@vinder/vinderjs",
  "version": "1.6.7",
  "description": "JS utilities used by Vinder Media Engineering",
  "license": "MIT",
  "repository": "sizeight/vinderjs",
  "main": "cjs/index.js",
  "module": "es/index.js",
  "keywords": [
    "react",
    "FontAwesome"
  ],
  "author": "Herman Pretorius (https://github.com/sizeight)",
  "browserslist": {
    "production": [
      ">0.2%",
      "ie >= 11",
      "not dead"
    ],
    "development": [
